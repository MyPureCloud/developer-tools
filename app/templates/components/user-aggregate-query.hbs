{{analytics-query-interval value=interval}}
{{analytics-query-granularity value=granularity}}

{{analytics-query-metrics selectedMetrics=selectedMetrics query="userAggregate"}}

<div class="panel panel-default">
    <div class="panel-heading"><h3 class="panel-title panel-title-condensed">Filter</h3></div>
    <div class="panel-body">
      <div class="filter-section">
        {{analytics-query-filter filter=filter filterValueOverride=userFilters}}
      </div>
    </div>
</div>
